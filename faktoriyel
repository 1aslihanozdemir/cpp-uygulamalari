#include <iostream>

using namespace std;

int sonuc = 1;

int donguileFaktoriyel(int sayi) //fonksiyon
{
    for(int i=2;i<=sayi;i++)
    {
        sonuc = sonuc*i;
    }
    return sonuc;
}


int main()
{   
    int girilenSayi;
    cout << "sayi: ";
    cin >> girilenSayi;
    cout << "faktoriyel("<< girilenSayi <<") = " << donguileFaktoriyel(girilenSayi);
    
}
